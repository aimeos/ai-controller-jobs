{
	"name": "aimeos/ai-controller-jobs",
	"description": "Aimeos job controllers for long running tasks",
	"keywords": ["aimeos", "extension", "controller", "jobs"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-controller-jobs",
		"issues": "https://github.com/aimeos/ai-controller-jobs/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.0.11",
		"aimeos/aimeos-core": "2023.10.*",
		"tecnickcom/tcpdf": "^6.4.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.0||~10.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	}
}